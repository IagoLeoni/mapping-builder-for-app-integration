{
  "version": "1.0",
  "description": "Schema padrão dos webhooks da Gupy",
  "lastUpdated": "2024-01-01",
  "schema": {
    "companyName": {
      "type": "string",
      "description": "Nome da empresa no sistema Gupy",
      "example": "ACME Corp",
      "semanticTags": ["company", "organization", "empresa", "companhia", "corporacao"]
    },
    "id": {
      "type": "string",
      "description": "Identificador único do evento",
      "example": "evt_123456",
      "semanticTags": ["id", "identifier", "uuid", "eventId", "event_id"]
    },
    "event": {
      "type": "string",
      "description": "Tipo do evento disparado",
      "example": "pre-employee.moved",
      "semanticTags": ["event", "action", "evento", "tipo", "type", "eventType"]
    },
    "date": {
      "type": "string",
      "format": "ISO-8601",
      "description": "Data e hora do evento",
      "example": "2024-01-01T10:00:00Z",
      "semanticTags": ["date", "timestamp", "data", "datetime", "created_at", "updated_at"]
    },
    "data.candidate.name": {
      "type": "string",
      "description": "Primeiro nome do candidato",
      "example": "João",
      "semanticTags": ["name", "firstName", "nome", "primeiroNome", "given_name", "first_name"]
    },
    "data.candidate.lastName": {
      "type": "string",
      "description": "Sobrenome do candidato",
      "example": "Silva",
      "semanticTags": ["lastName", "surname", "sobrenome", "ultimoNome", "family_name", "last_name"]
    },
    "data.candidate.email": {
      "type": "string",
      "format": "email",
      "description": "Email do candidato",
      "example": "joao@email.com",
      "semanticTags": ["email", "mail", "emailAddress", "email_address", "e_mail", "correio"]
    }
  }
}
