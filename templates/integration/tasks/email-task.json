{
  "task": "EmailTask",
  "taskId": "{{TASK_ID}}",
  "parameters": {
    "To": {
      "key": "To",
      "value": {
        "stringArray": {
          "stringValues": ["{{EMAIL_TO}}"]
        }
      }
    },
    "Subject": {
      "key": "Subject",
      "value": {
        "stringValue": "{{EMAIL_SUBJECT}}"
      }
    },
    "TextBody": {
      "key": "TextBody",
      "value": {
        "stringValue": "{{EMAIL_BODY}}"
      }
    }
  },
  "taskExecutionStrategy": "WHEN_ANY_SUCCEED",
  "displayName": "{{DISPLAY_NAME}}",
  "externalTaskType": "NORMAL_TASK",
  "position": { "x": "{{POSITION_X}}", "y": "{{POSITION_Y}}" }
}
