      "key": "connectionName",
      "value": {
        "stringValue": "{{PUBSUB_CONNECTION}}"
      }
    },
    "actionName": {
      "key": "actionName", 
      "value": {
        "stringValue": "publishMessage"
      }
    },
    "operation": {
      "key": "operation",
      "value": {
        "stringValue": "EXECUTE_ACTION"
      }
    },
    "connectionVersion": {
      "key": "connectionVersion",
